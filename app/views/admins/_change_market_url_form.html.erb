<%= form_for( @admin, :url => change_market_url_admin_path(@admin) ) do |f| %>
  <% if @admin.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@admin.errors.count, "error") %> prohibited this admin from being saved:</h2>

      <ul>
      <% @admin.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  <% f.hidden_field :name, :value => @admin.name %>
  <% f.hidden_field :right_sig_url, :value => @admin.right_sig_url %>
  <% f.hidden_field :phone, :value => @admin.phone %>
  <% f.hidden_field :fax, :value => @admin.fax %>
  <% f.hidden_field :password, :value => @admin.password %>
  <% f.hidden_field :password_confirmation, :value => @admin.password_confirmation %>
  <% f.hidden_field :password_digest, :value => @admin.password_digest %>
  <% f.hidden_field :email, :value => @admin.email %>
  <div class="field">
     <%= f.label 'Marketplace URL' %><br>
     <%= f.text_field :mkt_place_url %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
